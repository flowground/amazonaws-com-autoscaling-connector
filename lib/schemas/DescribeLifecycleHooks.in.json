{
    "type": "object",
    "properties": {
        "Action": {
            "required": true,
            "type": "string"
        },
        "Version": {
            "required": true,
            "type": "string"
        },
        "requestBody": {
            "example": {
                "AutoScalingGroupName": "my-auto-scaling-group"
            },
            "properties": {
                "AutoScalingGroupName": {
                    "description": "The name of the Auto Scaling group.",
                    "maxLength": 1600,
                    "minLength": 1,
                    "type": "string",
                    "x-pattern": "[\\u0020-\\uD7FF\\uE000-\\uFFFD\\uD800\\uDC00-\\uDBFF\\uDFFF\\r\\n\\t]*"
                },
                "LifecycleHookNames": {
                    "description": "The names of one or more lifecycle hooks. If you omit this parameter, all lifecycle hooks are described.",
                    "items": {
                        "maxLength": 255,
                        "minLength": 1,
                        "pattern": "[A-Za-z0-9\\-_\\/]+",
                        "type": "string"
                    },
                    "maxItems": 50,
                    "type": "array"
                }
            },
            "required": [
                "AutoScalingGroupName"
            ],
            "type": "object"
        },
        "X_Amz_Content_Sha256": {
            "required": false,
            "type": "string"
        },
        "X_Amz_Date": {
            "required": false,
            "type": "string"
        },
        "X_Amz_Algorithm": {
            "required": false,
            "type": "string"
        },
        "X_Amz_Credential": {
            "required": false,
            "type": "string"
        },
        "X_Amz_Security_Token": {
            "required": false,
            "type": "string"
        },
        "X_Amz_Signature": {
            "required": false,
            "type": "string"
        },
        "X_Amz_SignedHeaders": {
            "required": false,
            "type": "string"
        }
    }
}