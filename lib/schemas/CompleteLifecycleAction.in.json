                    "minLength": 36,
                    "type": "string"
                },
                "LifecycleHookName": {
                    "description": "The name of the lifecycle hook.",
                    "maxLength": 255,
                    "minLength": 1,
                    "pattern": "[A-Za-z0-9\\-_\\/]+",
                    "type": "string"
                }
            },
            "required": [
